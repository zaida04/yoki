{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "removeComments": true,
    "pretty": true,
    "module": "CommonJS",
    "target": "es2019",
    "lib": [
      "ESNext"
    ],
    "sourceMap": true,
    "incremental": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitHelpers": true,
    "resolveJsonModule": true,
    "importHelpers": true
  },
  "exclude": ["node_modules/**", "dist/**"]
}