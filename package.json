{
  "name": "yoki",
  "version": "1.0.0",
  "description": "A template for Akairo based TS Discord.js Bots",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "start:bot": "npm run build && node ./packages/bot/dist/index.js --trace-warnings",
    "start:dev": "npm run precheck && npm run start:bot",
    "start:dash": "node dist/packages/dashboard/index.js",
    "precheck": "rimraf packages/bot/dist && rimraf libs/logger/dist && rimraf packages/bot/dist",
    "build": "pnpm recursive run build --filter @yoki/*",
    "preinstall": "npx only-allow pnpm",
    "knex:init": "knex migrate:latest production",
    "lint": "npx eslint packages/ libs/ --fix",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaida04/Yoki.git"
  },
  "author": "Zaid",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zaida04/Yoki/issues"
  },
  "homepage": "https://github.com/zaida04/Yoki#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-marine": "^7.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^5.0.6",
    "nodemon": "^2.0.6",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/common-tags": "^1.8.0",
    "knex": "^0.21.14",
    "mysql": "^2.18.1"
  }
}
